 <!DOCTYPE html>
 <html lang="en">
 <head>
     <meta charset="UTF-8">
     <meta name="viewport" 
     	content="width=device-width, initial-scale=1.0">
     <meta http-equiv="X-UA-Compatible" content="ie=edge">
     <title>First Express</title>
 </head>
 <body>
   <h1>Pokemon</h1>
   <ul>
     <% pokemon.forEach(function(p) { %>
       <li>
         <%= p.name %>
           - 
         <%= p.favorite ? 'Favorite!' : 'Please go away' %>
       </li>
     <% }); %>
   </ul>
 </body>
 </html>
